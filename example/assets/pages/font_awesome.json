{
  "type": "set_value",
  "args": {
    "template": {
      "type": "material",
      "args": {
        "borderRadius": 8,
        "color": "#333"
      },
      "child": {
        "type": "padding",
        "args": {
          "padding": 16
        },
        "child": {
          "type": "column",
          "args": {
            "crossAxisAlignment": "center",
            "mainAxisAlignment": "center",
            "mainAxisSize": "max"
          },
          "children": [
            {
              "type": "fa_icon",
              "args": {
                "color": "#fff",
                "icon": "${value}",
                "size": 40.0
              }
            },
            {
              "type": "sized_box",
              "args": {
                "height": 16.0
              }
            },
            {
              "type": "text",
              "args": {
                "text": "${value}",
                "style": {
                  "color": "#fff"
                }
              }
            }
          ]
        }
      }
    }
  },
  "child": {
    "type": "scaffold",
    "args": {
      "appBar": {
        "type": "app_bar",
        "args": {
          "title": {
            "type": "text",
            "args": {
              "text": "Font Awesome Icon"
            }
          }
        }
      },
      "body": {
        "type": "grid_view",
        "args": {
          "gridDelegate": {
            "alignment": "center",
            "crossAxisSpacing": 16.0,
            "mainAxisSpacing": 16.0,
            "maxCrossAxisExtent": 200,
            "type": "max_cross_axis_extent"
          },
          "padding": 16.0
        },
        "children": "${for_each(icons, 'template')}"
      }
    }
  }
}